{
  "name": "lab1",
  "version": "1.0.0",
  "description": "lab1",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node build/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q build/index.js\"",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "format": "npx prettier src/ --check",
    "format:fix": "npx prettier src/ --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rostyslav27/node-lab1.git"
  },
  "author": "IO-13 Rostyslav Kunchiy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rostyslav27/node-lab1/issues"
  },
  "homepage": "https://github.com/Rostyslav27/node-lab1#readme",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.2",
    "prettier": "3.1.1",
    "typescript": "^5.3.3"
  }
}
